{
  "build": {
    "env": {
      "ENABLE_FILE_SYSTEM_API": "1"
    }
  },
  "github": {
    "enabled": false
  },
  "redirects": [
    { "source": "/:year/players", "destination": "/:year/apps", "permanent": true },
    { "source": "/teams", "destination": "/rivals", "permanent": true },
    { "source": "/teams/:slug", "destination": "/rivals/:slug", "permanent": true}
  ]
}
